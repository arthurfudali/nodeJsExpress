<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Pedidos</title>
  </head>
  <body>
    <h1>Pedidos</h1>
    <% %> <%= pedidos %>
    <h4>foreach sem tablea:</h4>
    <table border="1">
      <tr>
        <th width="150px">Produto</th>
        <th>Preco:</th>
      </tr>
      <% pedidos.forEach((pedido) => { %>
      <tr>
        <th>produto: <%= pedido.produto%></th>
        <th>Preco: <%= pedido.valor%></th>
      </tr>
      <% }) %>
    </table>
  </body>
</html>
